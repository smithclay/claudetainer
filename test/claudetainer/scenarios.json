{
  "test_base_only": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {}
    }
  },
  "test_base_with_python": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {
        "include": "python"
      }
    }
  },
  "test_tmux_multiplexer": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {
        "multiplexer": "tmux"
      }
    }
  },
  "test_zellij_multiplexer": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {
        "multiplexer": "zellij"
      }
    }
  },
  "test_none_multiplexer": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {
        "multiplexer": "none"
      }
    }
  },
  "test_zellij_compact_layout": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {
        "multiplexer": "zellij",
        "zellij_layout": "phone"
      }
    }
  },
  "test_workspace_navigation": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {
        "include": "base"
      }
    }
  },
  "test_ssh_configuration": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {},
      "ghcr.io/devcontainers/features/sshd:1": {
        "SSHD_PORT": 2222,
        "START_SSHD": "true",
        "USERNAME": "node",
        "NEW_PASSWORD": "node"
      }
    }
  },
  "test_ssh_key_generation": {
    "image": "mcr.microsoft.com/devcontainers/javascript-node:22-bookworm",
    "user": "node",
    "features": {
      "claudetainer": {}
    }
  }
